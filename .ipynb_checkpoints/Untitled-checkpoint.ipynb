    "def trapezoid(f,a,b,n):\n",
    "    h=(b-a)/float(n)\n",
    "    s=0.5*(f(a)+f(b))\n",
    "    for i in range(1,n,1):\n",
    "        s=s + f(a + i*h)\n",
    "    return h*s\n",
    "\n",
    "import math \n",
    "def g(t):\n",
    "    return math.sin(t)\n",
    "\n",
    "a = 0;  b = 1\n",
    "n = 1\n",
    "result = trapezoid(g, a, b, n)\n",
